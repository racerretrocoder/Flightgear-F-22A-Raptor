 <animation>
    <type>select</type>
    <object-name>machCone</object-name>
    <condition>
      <greater-than>
        <property>/environment/relative-humidity</property>
        <value>50</value>
      </greater-than>
      <greater-than>
        <property>/environment/temperature-degc</property>
        <value>0</value>
      </greater-than>
      <greater-than>
       <property>velocities/mach</property><!-- velocities/mach -->
       <value>0.98</value>
      </greater-than>
      <less-than>
       <property>velocities/mach</property><!-- velocities/mach -->
       <value>1.0</value>
      </less-than>
      <greater-than>
       <property>/environment/pressure-inhg</property>
       <value>22</value>
      </greater-than>
    </condition>
  </animation>


</effect>
<!--Y is Left to right
X is In and Out 
Z is Up and down-->

 <model>
  <name>AIM-120smoke</name>
  <path>Aircraft/F-22/Models/smoke/smoke2.xml</path>
  <offsets>
   <x-m>8</x-m>
   <y-m>0.0  </y-m>
   <z-m>0.0  </z-m>
  </offsets>
 </model>

<!--missile fire-->
	<model>

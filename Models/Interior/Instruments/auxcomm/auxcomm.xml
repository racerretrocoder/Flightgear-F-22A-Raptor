<?xml version="1.0"?>
<!-- comms panel -->

<PropertyList>

 <!--  Front HUD computer -->
   <animation>
  <type>material</type>
  <object-name>auxfreq</object-name>
  <ambient>
    <red>0.0</red>
    <green>1.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.0</red>
    <green>0.1</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.3</red>
    <green>0.8</green>
    <blue>0.3</blue>
  </emission> 
 </animation>
 
  <text>
  <name>auxfreq</name>
  <font>lcd.txf</font>
  <character-size type="double">0.007</character-size>
  <offsets>
				<x-m>-4.3298</x-m>
					<y-m>0.409</y-m>
				<z-m>0.546</z-m>
                <pitch-deg>-90</pitch-deg>
                <roll-deg>11</roll-deg>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">number-value</type>
  <format type="string">%3.3f</format>
  <truncate type="bool">false</truncate>
  <property>instrumentation/comm[2]/frequencies/selected-mhz</property>
  <draw-text type="bool">true</draw-text>
  <alignment>right-top</alignment>
 </text>

</PropertyList>
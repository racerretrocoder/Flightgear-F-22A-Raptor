<?xml version="1.0"?>

<!-- comms panel -->

<PropertyList>


   <model>
		<path>bg.ac</path>

	</model>


 	 <animation>
  <type>material</type>
  <object-name>fuel-head</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.0</red>
    <green>0.1</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.0</red>
    <green>0.8</green>
    <blue>0.0</blue>
  </emission> 
 </animation>

 	 <animation>
  <type>material</type>
  <object-name>red warn</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.1</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>1.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </emission> 
 </animation>

 	 <animation>
  <type>material</type>
    <object-name>mastcaut</object-name>
  <object-name>yell warn</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.1</red>
    <green>0.1</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>0.0</blue>
  </emission> 
 </animation>


   <animation>
    <type>select</type>
    <object-name>mastcaut</object-name> <!--Low red-->
    <condition>
     <and>
      <equals>
       <property>f22/blink</property>
       <value>1</value>
      </equals>
      <equals>
       <property>f22/caution</property>
       <value>1</value>
      </equals>
      </and>
    </condition>
  </animation>




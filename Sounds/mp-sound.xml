<?xml version="1.0"?>
<PropertyList>
<fx>
<!-- External Sounds --> 
<!-- Afterburner -->
     <engine>
        <name>left afterburner</name>
        <path>/Aircraft/F-22/Sounds/AB.wav</path>
        <mode>looped</mode>
        <condition>
             <not>
          <property>/sim/current-view/internal</property>
     </not>
<greater-than>  
 <property>sim/multiplay/generic/float[34]</property>
   <value>90</value>
  </greater-than>
  
<less-than>
    <property>sim/multiplay/generic/float[30]</property>
    <value>1</value>
</less-than>
<less-than>
    <property>orientation/pitch-deg</property>
    <value>30</value>
</less-than>
<greater-than>
    <property>orientation/pitch-deg</property>
    <value>-30</value>
</greater-than>
        </condition>
        <volume>
            <property>sim/multiplay/generic/float[31]</property>
            <offset>1</offset>
            <factor>0</factor>
            <min>0.0</min>
            <max>0.5</max>
        </volume>
           <pitch>
    	<property>sim/multiplay/generic/float[31]</property>
	<factor>0.005</factor>
	<min>0.0</min>
	<max>0.0</max>
   </pitch>
        <position>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>600.0</reference-dist>
        <max-dist>6000.0</max-dist>
    </engine>

     <engine>
        <name>left afterburner2</name>
        <path>/Aircraft/F-22/Sounds/AB.wav</path>
        <mode>looped</mode>
        <condition>
<greater-than>  
 <property>sim/multiplay/generic/float[34]</property>
